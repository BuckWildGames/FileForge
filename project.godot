; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="File Forge"
config/description="A tool for creating json files."
config/version="0.1.0"
run/main_scene="res://scenes/main.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="File Forge"
config/features=PackedStringArray("4.3", "Forward Plus")
boot_splash/image="res://sprites/logo.png"
config/icon="res://sprites/logo.png"

[autoload]

Global="*res://scripts/global.gd"
Ask="*res://scenes/ask.tscn"
GlobalFileDialog="*res://scenes/file_dialog.tscn"
Config="*res://scripts/config.gd"
Settings="*res://scenes/settings.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/borderless=true
window/size/window_width_override=1487
window/size/window_height_override=837
window/stretch/mode="canvas_items"

[gui]

theme/custom="res://theme/theme.tres"
theme/custom_font="res://theme/font.ttf"
